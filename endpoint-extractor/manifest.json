{
    "manifest_version": 3,
    "name": "Endpoint Extractor",
    "version": "1.0",
    "description": "Extracts all endpoints from the current page and external scripts.",
    "permissions": ["scripting", "activeTab"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon12.png",
        "48": "icons/icon64.png",
        "128": "icons/icon128.png"
      }
    },
    "icons": {
      "16": "icons/icon12.png",
      "48": "icons/icon64.png",
      "128": "icons/icon128.png"
    },
    "host_permissions": ["<all_urls>"]
  }